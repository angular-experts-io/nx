    "inquirer-autocomplete-prompt": "^2.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nrwl/angular": "^14.1.7",
    "@nrwl/cli": "14.1.7",
    "@nrwl/devkit": "14.1.7",
    "@nrwl/eslint-plugin-nx": "14.1.7",
    "@nrwl/jest": "14.1.7",
    "@nrwl/js": "14.1.7",
    "@nrwl/linter": "14.1.7",
    "@nrwl/nx-plugin": "^14.1.7",
    "@nrwl/workspace": "14.1.7",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
