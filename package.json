    "jest": "27.5.1",
    "nx": "14.1.7",
    "prettier": "^2.5.1",
    "replace-json-property": "^1.8.0",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "tslib": "^2.0.0",
    "typescript": "~4.6.2"
